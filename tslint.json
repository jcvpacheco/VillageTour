{
    "extends": [
      "tslint:recommended",
      "tslint-react",
      "tslint-config-prettier"
    ],
    "rules": {
      "jsx-no-lambda": false,
      "ordered-imports": false,
      "member-access":false
    },
    "linterOptions": {
        "exclude": [
        "config/**/*.js",
        "node_modules/**/*.ts",
        "coverage/lcov-report/*.js",
        "src/components/*.tsx"
        ]
  }
}